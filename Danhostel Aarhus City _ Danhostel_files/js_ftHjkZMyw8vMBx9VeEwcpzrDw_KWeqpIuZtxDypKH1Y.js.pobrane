(function ($, Drupal, drupalSettings) {

  /**
   * Initialize datepicker for dh_datepicker render element type.
   */
  Drupal.behaviors.dh_datepicker_element = {
    attach: function (context, settings) {
      // Search all datepickers once.
      var $datepickers = $(context).find('.dh-datepicker').once('dh-datepicker');
      // Initialize datepickers for each dh_datepicker form field.
      $datepickers.each(function () {
        $(this).datepicker({
          dateFormat: 'dd-mm-yy',
          showButtonPanel: false,
          changeYear: true,
          changeMonth : true,
          yearRange: "-3:+3",
          minDate: 0,
          closeAtTop: false,
          defaultDate: "0y",
          speed: "immediate",
          fromTo: false,
          autoPopUp: "focus",
          firstDay: 1
        });
      });
    }
  };

  /**
   * Automatic set "end date" after "date from" set.
   */
  Drupal.behaviors.dh_datepicker_start_end_date = {
    attach: function (context, settings) {
      // set to date when from date selected.
      // prepare variables. get datepickers.
      var $datepickerFrom = $(context).find('.form-item-date-from .dh-datepicker');
      var $datepickerTo = $(context).find('.form-item-date-to .dh-datepicker');
      // Add onSelect function to first datepicker.
      $datepickerFrom.datepicker( "option", { onSelect: function (selectedDate) {
        // Add one day to selected date.
        var selectedDateFormatted = $.datepicker.parseDate("dd-mm-yy", selectedDate);
        selectedDate = new Date(selectedDateFormatted);
        selectedDate.setDate(selectedDate.getDate() + 1);
        // Set date value to second datepicker.
        if (!$datepickerTo.val() || selectedDateFormatted > $.datepicker.parseDate("dd-mm-yy", $datepickerTo.val())) {
          $datepickerTo.datepicker( "setDate", selectedDate );
        }
      }});
    }
  };
})(jQuery, Drupal, drupalSettings);
