(function ($, Drupal) {

  /**
   * This behavior is created to change click behaviour of main menu links.
   */
  Drupal.behaviors.dh_main_menu = {
    attach: function (context, settings) {

      var $menuItems = $(context).find('#block-danhostelhoved ul.menu > li');

      $menuItems.each(function () {
        $(this).click(function(){
            window.location=$(this).find("a").attr("href");
            return false;
          }
        );
      });

    }
  };

  // top menu mobile

  Drupal.behaviors.dh_menu_mobile = {
    attach: function (context, settings, drupalSettings) {
      $(context).find('.region-navigation-top').once('navigation-mobile').each(function () {
        var navButton = '<div class="nav-toggle-button"><span></span><span></span><span></span></div>';
        var header = $('.region.region-navigation-top');

        header.append(navButton);

        $('.nav-toggle-button').on('click', function () {
          header.toggleClass('active');
          $('body').toggleClass('no-scroll');
        });

      });
    }
  };

  // footer links mobile

  Drupal.behaviors.dh_footer_links_mobile = {
    attach: function (context, settings, drupalSettings) {
      $(context).find('.dh-footer').once('footer-mobile').each(function () {
        var linksWrap = $('.dh-footer-links-wrap');
        var linksArrow = '<div class="arrow-panel"><span class="arrow"></span></div>';
        // var accordionArrow = $('.accordion-header span.arrow');

        linksWrap.append(linksArrow);

        $('.dh-footer-links-wrap .arrow-panel').on('click', function () {
          linksWrap.toggleClass('show');
        });
      });
    }
  };

  // main-menu mobile

  Drupal.behaviors.dh_main_menu_mobile = {
    attach: function (context, settings, drupalSettings) {
      // var nav = $('nav .dh-main-menu-wrapper');
      $(context).find('.dh-main-menu-wrapper').once('accordion-mobile').each(function () {
        var accordionHolder = $('nav .dh-main-menu-wrapper .menu.nav');
        var accordionHeader = '<div class="accordion-header"><span class="accordion-title">' + Drupal.t('Choose type of stay') + '</span><span class="arrow"></span></div>';
        // var accordionArrow = $('.accordion-header span.arrow');

        accordionHolder.before(accordionHeader);

        $('.dh-main-menu-wrapper').on('click', function () {
          accordionHolder.toggleClass('active');
          $('.accordion-header').toggleClass('active');
        });
      });
    }
  };

  Drupal.behaviors.dh_vertical_menu_mobile = {
    attach: function (context, settings, drupalSettings) {

      $(context).find('.dh-vertical-menu').once('vertical-menu').each(function () {

          var verticalMenuNav = $('.dh-vertical-menu ul.menu.nav li').clone();
          var velricalMenuActive = $('.dh-vertical-menu ul.menu.nav li.active').clone();
          var ddHolder = '<div class="select-wrapper vertical-menu"><div class="placeholder form-select form-control">Menu</div><ul class="vertical-menu-dd"></ul></div>';
          $(this).before(ddHolder);
          var ddSelect = $('.select-wrapper.vertical-menu ul.vertical-menu-dd');
          var placeholder = $('.select-wrapper.vertical-menu .placeholder');
          console.log(velricalMenuActive);
          ddSelect.append(verticalMenuNav);

          placeholder.on('click', function () {
            $('.select-wrapper.vertical-menu .vertical-menu-dd').toggleClass('active');
          });


      });
    }
  };

})(jQuery, Drupal);
