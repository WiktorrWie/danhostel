(function ($, Drupal, drupalSettings) {

    /**
     * Add auto redirection to search form.
     */
    Drupal.behaviors.dh_subpages_autosubmit = {
        attach: function (context, settings, drupalSettings) {
            if (settings.booking_options_redirects) {
                $(context).find('.js-hostel-subpages-search-type').change(function () {
                    if (settings.booking_options_redirects[$(this).val()]) {
                        window.location = settings.booking_options_redirects[$(this).val()];
                    }
                });
            }
        }
    };

})(jQuery, Drupal, drupalSettings);
